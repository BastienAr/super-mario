{
  "name": "warp-pipes",
  "description": "Nodejs stream utility",
  "license": "MIT",
  "author": "Banst <bastyen.a@gmail.com>",
  "repository": "https://github.com/BastienAr/warp-pipes.git",
  "version": "0.2.0",
  "main": "lib/index.js",
  "files": [
    "lib/"
  ],
  "scripts": {
    "build": "run-s clean build:*",
    "build:ts": "tsc",
    "clean": "rm -rf lib coverage .nyc_output yarn-error.log test-result.cml lint-result.xml",
    "precommit": "lint-staged",
    "coverage": "nyc check-coverage --lines 100 --functions 100 --branches 100 && nyc report --reporter=cobertura",
    "lint": "tslint --project ./tsconfig.json src/**/*.ts",
    "nyc-report": "nyc report",
    "prepublish": "yarn run build",
    "test": "cross-env NODE_ENV=test nyc mocha"
  },
  "devDependencies": {
    "@types/chai": "^4.1.3",
    "@types/chance": "^1.0.0",
    "@types/event-to-promise": "^0.7.0",
    "@types/lodash": "^4.14.109",
    "@types/mocha": "^5.2.1",
    "@types/node": "^10.3.2",
    "@types/sinon": "^5.0.1",
    "chai": "4.1.2",
    "chance": "1.0.16",
    "cross-env": "5.2.0",
    "event-to-promise": "0.8.0",
    "husky": "0.14.3",
    "lint-staged": "7.2.1",
    "lodash": "4.17.10",
    "mocha": "5.2.0",
    "npm-run-all": "4.1.3",
    "nyc": "12.0.2",
    "prettier": "1.14.1",
    "prettier-package-json": "1.6.0",
    "sinon": "6.0.1",
    "stream-mock": "1.2.0",
    "ts-node": "^7.0.0",
    "tslint": "^5.10.0",
    "tslint-microsoft-contrib": "^5.0.3",
    "typescript": "^2.9.1"
  },
  "keywords": [
    "fork",
    "map",
    "pipe",
    "split",
    "stream",
    "transform",
    "util"
  ],
  "nyc": {
    "require": [
      "ts-node/register"
    ],
    "extension": [
      ".ts"
    ],
    "reporter": [
      "lcov",
      "text"
    ],
    "sourceMap": true,
    "instrument": true,
    "report-dir": "coverage"
  },
  "types": "lib/index.d.ts"
}
